		Interp 'Stop
			(H
				(Int -> CPS
					(H
						(Int -> CPS (HV String) (HV String)
							(InterpV 'PropShift (HV Bool) (HV String) (HV String)))
						((Int -> CPS (HV String) (HV String) Bool) ->
							CPS (HV String) (HV String) String))
					(HV Bool) Int)
				(
					(Int -> CPS
						(H
							(Int -> CPS (HV String) (HV String)
								(InterpV 'PropShift (HV Bool) (HV String) (HV String)))
